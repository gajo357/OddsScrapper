@model ContactInfoModel
@{
    ViewData["Title"] = "Contact";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<form method="post">
    <label asp-for="Name"></label>
    <input asp-for="Name"/>
    <label asp-for="Email"></label>
    <input asp-for="Email" />
    <div>
        <input type="submit" value="Send"/>
    </div>
</form>
